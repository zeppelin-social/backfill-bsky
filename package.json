{
	"name": "backfill-bsky",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"backfill": "bun --env-file=.env backfill/main.ts",
		"after-backfill": "bun --env-file=.env scripts/afterBackfill.ts",
		"buffer": "deno run --env-file=.env -A buffer/main.ts",
		"buffer:ingest": "deno run --v8-flags=--max-old-space-size=65536 --env-file=.env -A buffer/ingest.ts",
		"request-crawl": "bun --env-file=.env scripts/requestCrawlPdses.ts",
		"index-repos": "bun --env-file=.env scripts/indexRepos.ts",
		"index-profiles": "bun --env-file=.env scripts/indexProfiles.ts",
		"create-indexes": "bun --env-file=.env scripts/createIndexes.ts",
		"drop-indexes": "bun --env-file=.env scripts/dropIndexes.ts",
		"fetch-dids": "bun --env-file=.env scripts/fetchDids.ts"
	},
	"keywords": [],
	"author": "",
	"license": "MPL-2.0",
	"devDependencies": {
		"@types/bun": "^1.1.14",
		"@types/node": "^22.9.4",
		"@types/pg": "^8.11.10",
		"dprint": "^0.47.5",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@atcute/bluesky": "^3.1.5",
		"@atcute/car": "^3.0.5",
		"@atcute/cbor": "^2.2.4",
		"@atcute/client": "^2.0.7",
		"@atcute/lexicons": "^1.1.0",
		"@atcute/tid": "^1.0.2",
		"@atproto/identity": "^0.4.8",
		"@atproto/lexicon": "^0.4.11",
		"@atproto/repo": "^0.6.4",
		"@atproto/sync": "^0.1.13",
		"@atproto/syntax": "^0.3.2",
		"@opensearch-project/opensearch": "^3.5.1",
		"@redis/client": "^1.6.0",
		"@zeppelin-social/bsky-backfill": "0.0.171",
		"bee-queue": "^1.7.1",
		"cacheable-lookup": "^7.0.0",
		"kysely": "^0.22.0",
		"large-map": "^1.0.0",
		"lru-cache": "^11.1.0",
		"multiformats": "^13.3.1",
		"normalize-url": "^8.0.2",
		"p-queue": "^8.0.1",
		"pg": "^8.13.1",
		"undici": "^6.21.0"
	}
}
